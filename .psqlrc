\set QUIET 1
\pset null '[NULL]'
\set PROMPT1 '%[%033[1;30m%][%[%033[0m%] %[%033[1;97m%]psql%[%033[0m%]%[%033[1;30m%]://%[%033[0m%]%[%033[1;32m%]%n@%M%[%033[0m%]%[%033[1;30m%]:%[%033[0m%]%[%033[1;97m%]%>%[%033[0m%] %[%033[1;34m%]%/%[%033[0m%] %[%033[1;97m%]%R%[%033[0m%]%[%033[1;30m%]]%[%033[0m%]\n%[%033[1;34m%]%#%[%033[0m%] '
\set PROMPT2 '    '
\timing
-- \x auto
-- \set VERBOSITY verbose
-- \set HISTFILE ~/.psql_history- :DBNAME
\set HISTCONTROL ignoredups
\set COMP_KEYWORD_CASE upper
\unset quiet
